FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/LyftSDK" "${PODS_CONFIGURATION_BUILD_DIR}/UberCore" "${PODS_CONFIGURATION_BUILD_DIR}/UberRides"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/LyftSDK/LyftSDK.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/UberCore/UberCore.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/UberRides/UberRides.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "LyftSDK" -framework "UberCore" -framework "UberRides"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
